	},
	"item": [
		{
			"name": "viacep-consulta",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"function zipCode() {\r",
							"\r",
							"    var zipCode = [\"98300-000\", \"01001-000\",\"69316-106\", \"68909-707\", \"91180-400\", \"59064-810\", \"60813-040\", \"69911-090\"];\r",
							"\r",
							"    var randomItem = zipCode[Math.floor(Math.random() * zipCode.length)];\r",
							"    return randomItem;\r",
							"}\r",
							"\r",
							"pm.variables.set(\"ZipCode\", zipCode());"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{url}}/{{ZipCode}}/json/",
					"host": [
						"{{url}}"
					],
					"path": [
						"{{ZipCode}}",
						"json",
						""
					]
				}
			},
			"response": []
		}
	]
}